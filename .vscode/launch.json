  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "gdb",
      "request": "launch",
      "target": "${fileDirname}/${fileBasenameNoExtension}",
      "cwd": "${workspaceFolder}",
      "valuesFormatting": "parseText",
      "gdbpath": "/usr/bin/gdb",
      "stopAtEntry": "_start"
    }
  ]
}